{
  "name": "gym-rat",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start-auth": "json-server --watch auth-db.json --port 3001",
    "start-classes": "json-server --watch class-db.json --port 3002",
    "start-plans": "json-server --watch plan-db.json --port 3003",
    "start-all": "concurrently \"npm run start-auth\" \"npm run start-classes\" \"npm run start-plans\""
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "concurrently": "^7.6.0",
    "vite": "^5.2.8"
  }
}
